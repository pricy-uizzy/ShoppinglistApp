{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      ["@babel/plugin-proposal-private-property-in-object", { "loose": true }],
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      ["styled-components", { "ssr": true, "displayName": true }]
    ]
  }